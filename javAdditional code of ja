// program to find the LCM of two integers

//take  input 
const num1 = promt("Enter a first positive integer: ");
const num2 = promt("Enter a second positive integer: ");

//higher number among number 1 und number 2 is stored in min
let min = (num1 > num2) ? num1 : num2;

// while loop
while () loop
  if (min % num1 == 0 && min % num2 == 0) {
  console.log("The LCM of ${num} and ${num2} is ${min}");
  
  }
min++;
}
